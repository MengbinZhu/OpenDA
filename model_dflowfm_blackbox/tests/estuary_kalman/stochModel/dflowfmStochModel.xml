            <noiseModel id="boundaryNoiseModelM2" className="org.openda.noiseModels.TimeSeriesNoiseModelFactory" workingDirectory=".">
                <configFile>BoundaryNoiseM2.xml</configFile>
                <exchangeItems>
                    <exchangeItem id="waterlevelnoise" operation="add" modelExchangeItemId="westboundary_M2.1:waterlevelbnd"/>
                </exchangeItems>
            </noiseModel>
            <!--noiseModel id="boundaryNoiseModelS2" className="org.openda.noiseModels.TimeSeriesNoiseModelFactory" workingDirectory=".">
                <configFile>BoundaryNoiseS2.xml</configFile>
                <exchangeItems>
                    <exchangeItem id="waterlevelnoise" operation="add" modelExchangeItemId="westboundary_S2.1:waterlevelbnd"/>
                </exchangeItems>
            </noiseModel-->
            <vector id="s1" ioObjectId="mapfile" elementId="s1"/>
            <vector id="unorm" ioObjectId="mapfile" elementId="unorm"/>
        </state>
        <predictor>
            <vector id="station01.waterlevel"/>
            <vector id="station02.waterlevel"/>
            <vector id="station03.waterlevel"/>
        </predictor>
    </vectorSpecification>

</blackBoxStochModel>
